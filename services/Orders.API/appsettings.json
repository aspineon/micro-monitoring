{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "writeTo": [
      { "Name": "Console" },
      {
        "Name": "Http",
        "Args": {
          "requestUri": "http://localhost:9601"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Orders"
    }
  },
  "AllowedHosts": "*"
}
